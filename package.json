{
  "name": "impulse-math-device-ts",
  "version": "0.1.0",
  "description": "Native memory: NVIDIA CUDA™ and RAM bindings for Node.js",
  "keywords": [
    "NVIDIA",
    "CUDA",
    "GPU"
  ],
  "homepage": "http://github.com/HudHatman/impulse-ml",
  "author": "Michał Baniowski",
  "contributors": [
    {
      "name": "Michał Baniowski",
      "email": "michal.baniowski@gmail.com"
    }
  ],
  "main": "dist/impulse-ml.js",
  "type": "commonjs",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "preinstall": "node-gyp clean || true; node-gyp configure build",
    "install": "node-gyp rebuild",
    "prepare": "npm run build-dev && npm run build-prod",
    "test": "node test/test.js",
    "build": "npm run build-prod && npm run build-dev",
    "build-dev": "node-gyp rebuild && webpack --config ./configs/dev.js",
    "build-prod": "webpack --config ./configs/prod.js",
    "prettier-write": "prettier --write \"src/typescript/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node14": "^1.0.3",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-loader": "^9.1.0",
    "eslint": "^8.30.0",
    "node-loader": "^2.0.0",
    "prettier": "^2.8.1",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "csvtojson": "^2.0.10"
  }
}
