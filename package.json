{
  "name": "impulse-math-device-ts",
  "version": "0.1.0",
  "description": "Native memory: NVIDIA CUDA™ and RAM bindings for Node.js",
  "keywords": [
    "NVIDIA",
    "CUDA",
    "GPU"
  ],
  "homepage": "http://github.com/HudHatman/impulse-ml",
  "author": "Michał Baniowski",
  "contributors": [
    {
      "name": "Michał Baniowski",
      "email": "michal.baniowski@gmail.com"
    }
  ],
  "main": "dist/impulse-ml.js",
  "type": "commonjs",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "preinstall": "node-gyp clean || true; node-gyp configure build",
    "install": "node-gyp rebuild",
    "build": "npm run build-prod && npm run build-dev",
    "build-dev": "webpack --config ./configs/dev.js",
    "build-prod": "webpack --config ./configs/prod.js",
    "prettier-write": "prettier --write \"src/typescript/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "node-loader": "^2.0.0",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "csvtojson": "^2.0.10"
  }
}
